<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rief">

    <application
        android:allowBackup="true"
        android:icon="@drawable/buku_clipart_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/buku_clipart_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AplikasiSederhana">
        <activity
            android:name=".view.DetailActivity"
            android:exported="false" />
        <activity
            android:name=".view.ListDataDetailActivity"
            android:exported="false" />
        <activity
            android:name=".view.ListDataActivity"
            android:exported="false" />
        <activity
            android:name=".view.AboutActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".SplashScreenActivity"
            android:exported="true"
            android:theme="@style/Theme.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>